<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="681dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="49dp">

        <Button
            android:id="@+id/button_async"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="164dp"
            android:text="Asyncrone" />

        <Button
            android:id="@+id/button_serialization"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sérialisation" />

        <Button
            android:id="@+id/button_graphql"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="48dp"
            android:text="GraphQL" />

        <Button
            android:id="@+id/button_diff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Différée" />

        <Button
            android:id="@+id/button_compress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Compressée" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>